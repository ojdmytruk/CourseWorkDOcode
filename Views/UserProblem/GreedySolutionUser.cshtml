@model CourseWorkDO.Models.SolutionMatrix
@{
    ViewBag.Title = "GreedySolutionUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<body>
    <div class="card m-5">
        <h2 class="card-header text-center">Жадібний розв'язок задачі</h2>
        <div class="card-body">
            <form class="p-a-1" action="/UserProblem/GreedySolutionUser" method="post">
                <div class="form-group">
                    @using (Html.BeginForm())
                    {
                        <table>
                            <tr>
                                <td>
                                    <h5>Номер місця призначення</h5>
                                </td>
                                @for (int column = 0; column < Model.SolutionArray.Length; column++)
                                {
                                    <td><h5>@(column+1)</h5></td>
                                }
                            </tr>
                            <tr>
                                <td>
                                    <h5>Номер об'єкта</h5>
                                </td>
                                @for (int column = 0; column < Model.SolutionArray.Length; column++)
                                {
                                    <td>@(Model.SolutionArray[column]+1)</td>
                                }
                            </tr>
                            <tr>
                                <td>
                                    <h5>Значення цільової функції</h5>
                                </td>
                                <td>
                                    @Model.Score
                                </td>
                            </tr>

                        </table>
                    }
                </div>

            </form>
        </div>
    </div>


</body>

